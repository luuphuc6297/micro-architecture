    "npmScope": "micro-architecture-coaching-cloud",
    "affected": {
        "defaultBase": "main"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"],
                "accessToken": "NjRkMGRhYTctZmNkYS00NmRhLWE5MGYtM2M1MDhlYzgyODUwfHJlYWQtd3JpdGU="
            }
        }
    },
    "targetDefaults": {
        "build": {
